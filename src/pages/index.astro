---
import BaseLayout from "../layouts/Layout.astro";
import Header from "../components/landing/Header.astro";
import Content from "../components/landing/Content.astro";
import { AstroSeo } from "@astrolib/seo";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
	title="Links by Shen Nan"
	description="Shen Nan | People, Purpose, Process"
	canonical="https://links.byshennan.com"
	openGraph={{
		url: "https://links.byshennan.com",
		title: "Bio",
		description: "Open Graph Description",
		images: [
			{
				url: "https://links.byshennan.com/images/avatar.jpg",
				width: 1200,
				height: 630,
				alt: "Shen Nan",
				type: "image/jpeg",
			},
			{
				url: "https://links.byshennan.com/images/avatar.jpg",
				width: 4096,
				height: 4096,
				alt: "Shen Nan",
				type: "image/jpeg",
			},
		],
		site_name: "Shen Nan | People, Purpose, Process",
	}}
	twitter={{
		handle: "@helloshennan",
		site: "@helloshennan",
		cardType: "summary_large_image",
	}}
/>
<BaseLayout>
	<Header />
	<Content />
</BaseLayout>
